            <stringProp name="script">
import java.time.Duration;
import java.time.Instant;
import java.time.LocalDateTime;
import java.time.ZoneOffset;
import java.time.format.DateTimeFormatter;

String incident = vars.get(&quot;EDR_INCIDENT&quot;);
//String pbName = vars.get(&quot;pbName&quot;);
String pbName = &quot;BT_Controller_Generic_Qradar&quot;;
log.info(pbName);

// Define the date format
DateTimeFormatter formatter = DateTimeFormatter.ofPattern(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss.SSSSSSSSSX&quot;);

// Define the start and end dates
String startTime = vars.get(&quot;conPBST&quot;);
String endTime = vars.get(&quot;tranPBST&quot;);
// Parse the start and end dates
Instant startInstant = LocalDateTime.parse(startTime, formatter).toInstant(ZoneOffset.UTC);
Instant endInstant = LocalDateTime.parse(endTime, formatter).toInstant(ZoneOffset.UTC);

// Calculate the time difference in seconds
long timeDifferenceSeconds = Duration.between(startInstant, endInstant).getSeconds();

// Print the time difference in seconds
log.info(&quot;Time Difference (in seconds): &quot; + timeDifferenceSeconds);

// Set the time difference to a JMeter variable
vars.put(&quot;ControllerPB&quot;, String.valueOf(timeDifferenceSeconds));
vars.put(&quot;pbName&quot;, pbName);

</stringProp>
